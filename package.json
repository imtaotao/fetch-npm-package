{
  "name": "fetch-npm-package",
  "version": "1.0.14",
  "description": "Use js to download and unzip the npm package",
  "main": "dist/fetch.cjs.js",
  "unpkg": "dist/fetch.umd.js",
  "module": "dist/fetch.esm-bundler.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rollup --config",
    "format": "prettier --write --parser typescript \"(src|dist)/**/*.(js|ts?(x))\""
